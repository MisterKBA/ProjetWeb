@model IEnumerable<NasaImage.Model.TodoDate>

@{
    ViewData["Title"] = "GetAllDate";
}

<h1>GetAllDate</h1>


<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                <a asp-action="Details" asp-route-date="@item.date">Vieuw</a>
            </td>
        </tr>
}
    </tbody>
</table>
